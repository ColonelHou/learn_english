    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "esbuild-wasm": "^0.19.12",
    "terser": "^5.44.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "overrides": {
    "esbuild": "npm:esbuild-wasm@^0.19.12"
  }
}
